{
  "active": true,
  "visible": true,
  "statusInclude": ["Review", "Released"],
  "pathnameTemplate": "DOC_${item.getId().getRelPath().getNameBase()}_${item.getProperties().getString(\"cms:status\")}.pdf",

  "options" : {
      "type": "abxpe",
      "format": "pdf",
      "params": {
          "stylesheet": "axdocbook.style",
          "pdfconfig": "smallfile.pdfcf"
      },
      "manifest": {
            "customTemplates": {
                "type": "${item.getMeta().get(\"embd_xml_a_type\").toString().toUpperCase()}"
            },
            "metaTemplates": {
                "type": "${item.getMeta().get(\"embd_xml_a_type\").toString()}"
            }
        },
      "storage": {
        "type": "s3"
        
      }
      
  }
}